{
  "name": "jenkins-api",
  "version": "0.3.1",
  "description": "Jenkins API written in Node.js",
  "author": "Shawn Jansepar <shawnjan@gmail.com>",
  "contributors": [
    "Jakub Podlaha <j.podlaha@gmail.com> (https://github.com/kub1x)"
  ],
  "license": "MIT",
  "keywords": [
    "github",
    "jenkins"
  ],
  "engine": {
